{
  "name": "webdriverio-typescript-samples",
  "version": "3.0.1",
  "description": "Provides examples of how to use TypeScript with the webdriver.io api. Tests executed against http://the-internet.herokuapp.com..",
  "main": "index.js",
  "scripts": {
    "test": "wdio wdio.conf.js",
    "lint": "tslint --fix src/**/*.ts",
    "moveJsonFiles": "copyfiles -u 1 src/selectors/* src/config/* lib",
   "build_tslint": "npm run lint && rimraf lib && npm run moveJsonFiles && tsc --outDir lib",
   "build": "rimraf lib && npm run moveJsonFiles && tsc --outDir lib"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdavis61/webdriverio-typescript-examples.git"
  },
  "keywords": [
    "webdriverio",
    "selenium",
    "nodejs",
    "ts",
    "typescript"
  ],
  "author": "James Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdavis61/webdriverio-typescript-examples/issues"
  },
  "homepage": "https://github.com/jdavis61/webdriverio-typescript-examples#readme",
  "dependencies": {
    "@types/mocha": "^8.2.1",
    "webdriverio": "^7.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@wdio/cli": "^7.1.2",
    "@wdio/dot-reporter": "^7.1.1",
    "@wdio/local-runner": "^7.1.2",
    "@wdio/mocha-framework": "^7.1.2",
    "@wdio/selenium-standalone-service": "^7.1.1",
    "@wdio/spec-reporter": "^7.1.1",
    "@wdio/sync": "^7.1.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "date-fns": "^2.22.1",
    "it-each": "^0.4.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.2.3"
  }
}
